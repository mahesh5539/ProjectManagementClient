function allowDrop(ev) {
    ev.preventDefault();
}

function drag(ev) {
    ev.dataTransfer.setData("Text", ev.target.id);
}

function drop(ev) {
// call func to change db state
// get the target node 
//   ev.target
var data = ev.dataTransfer.getData("Text");
var droptarget = ev.target.id;
var newStateStr ='';
if(droptarget=='first_column'){
    newStateStr = 'new';
}
if(droptarget=='second_column'){
    newStateStr = 'assigned';
}
if(droptarget=='third_column'){
    newStateStr = 'started';
}
if(droptarget=='fourth_column'){
    newStateStr = 'finished';
}
if(droptarget=='fifth_column'){
    newStateStr = 'cancelled';
}
//updateTaskState
// get current task//
//var data = ev.dataTransfer.getData("Text");
 // var abc = document.getElementById(data)
    updateTaskState(newStateStr,data);

    ev.target.appendChild(document.getElementById(data));
    ev.preventDefault();
}
</script>
